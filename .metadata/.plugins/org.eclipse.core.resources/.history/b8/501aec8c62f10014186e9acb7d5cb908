import java.util.Random;
public class BookInfo {

	public static String getRandBook(){
		String book;
		
		Random rand = new Random();
		int bookNum = rand.nextInt(39 - 1 + 1) + 1;
		
		switch (bookNum){
		case 1:
			book = "Genesis";
			break;
		case 2: 
			book = "Exodus";
			break;
		case 3: 
			book = "Leviticus";
			break;
		case 4:
			book = "Numbers";
			break;
		case 5: 
			book = "Deuteronomy";
			break;
		case 6: 
			book = "Joshua";
			break;
		case 7:
			book = "Judges";
			break;
		case 8: 
			book = "Ruth";
			break;
		case 9: 
			book = "1+Samuel";
			break;
		case 10:
			book = "Genesis";
			break;
		case 11:
			book = "gee";
		case 12: 
			book = "Exodus";
			break;
		case 13: 
			book = "Leviticus";
			break;
		case 14:
			book = "Genesis";
			break;
		case 15: 
			book = "Exodus";
			break;
		case 16: 
			book = "Leviticus";
			break;
		case 17:
			book = "Genesis";
			break;
		case 18: 
			book = "Exodus";
			break;
		case 19: 
			book = "Leviticus";
			break;
		case 20:
			book = "jjdjd";
			break;
		case 21:
			book = "Genesis";
			break;
		case 22: 
			book = "Exodus";
			break;
		case 23: 
			book = "Leviticus";
			break;
		case 24:
			book = "Genesis";
			break;
		case 25: 
			book = "Exodus";
			break;
		case 26: 
			book = "Leviticus";
			break;
		case 27:
			book = "Genesis";
			break;
		case 28: 
			book = "Exodus";
			break;
		case 29: 
			book = "Leviticus";
			break;
		case 30:
			book = "hhhh";
			break;
		case 31:
			book = "Genesis";
			break;
		case 32: 
			book = "Exodus";
			break;
		case 33: 
			book = "Leviticus";
			break;
		case 34:
			book = "Genesis";
			break;
		case 35: 
			book = "Exodus";
			break;
		case 36: 
			book = "Leviticus";
			break;
		case 37:
			book = "Genesis";
			break;
		case 38: 
			book = "Exodus";
			break;
		case 39: 
			book = "Leviticus";
			break;
			
		}
		
		return book;
	}
}
